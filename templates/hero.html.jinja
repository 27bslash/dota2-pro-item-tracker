<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Spider</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
  </head>
  <body>
    <form method="POST" action="">
      <input type="text" name="t" placeholder="Search.." />
      <input type="submit" placeholder="submit" value="submit" />
    </form>
    <img src='https://cdn.cloudflare.steamstatic.com/apps/dota2/images/heroes/{{hero_name}}_hphover.png?v=5926546'>
    <table>
      <tbody class="item-table">
        {% for dict_item in data %} {% for item in dict_item['items'] %} {%set
        img_name = item.key %} {% set time = item.time%}
        <tr>
          <td>{{ time }}</td>
          <td>
            <img
              src="https://cdn.cloudflare.steamstatic.com/apps/dota2/images/items/{{
                img_name
              }}_lg.png"
            />
          </td>
        </tr>
        {%endfor%} {% endfor %}
      </tbody>
    </table>
  </body>
</html>
