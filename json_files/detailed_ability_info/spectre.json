            "attributes": [
                "DAMAGE: 90 / 140 / 190 / 240",
                "MOVEMENT SPEED CHANGE: 10% / 14% / 18% / 22%",
                "DURATION: 7.0 / 7.0 / 7.0 / 7.0"
